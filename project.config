[project]
	description = Eclipse Git Team Provider (technology.egit)
[receive]
	requireContributorAgreement = true
[access "refs/heads/*"]
	forgeAuthor = group technology.egit-committers
	label-Code-Review = -2..+2 group technology.egit-committers
	label-IP-Clean = -1..+1 group technology.egit-committers
	label-Verified = -1..+1 group technology.egit-committers
	submit = group technology.egit-committers
	push = group Administrators
[access "refs/tags/*"]
	pushTag = group Administrators
